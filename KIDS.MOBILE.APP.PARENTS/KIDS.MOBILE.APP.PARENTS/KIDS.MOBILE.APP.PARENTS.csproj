<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Images\close.png" />
    <None Remove="Resources\Images\cover.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Images\close.png" />
    <EmbeddedResource Include="Resources\Images\cover.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Com.Airbnb.Xamarin.Forms.Lottie" Version="3.1.3" />
    <PackageReference Include="DLToolkit.Forms.Controls.FlowListView" Version="2.0.11" />
    <PackageReference Include="Microsoft.AppCenter.Analytics" Version="3.4.2" />
    <PackageReference Include="Microsoft.AppCenter.Crashes" Version="3.4.2" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Plugin.Badge" Version="2.3.0" />
    <PackageReference Include="Prism.Unity.Forms" Version="7.2.0.1422" />
    <PackageReference Include="Realm" Version="5.0.1" />
    <PackageReference Include="RestSharp" Version="106.11.5" />
    <PackageReference Include="Syncfusion.Xamarin.Buttons" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.Cards" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.Core" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.SfChart" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.SfComboBox" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.SfDataGrid" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.SfListView" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.SfPicker" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.SfProgressBar" Version="18.2.0.58" />
    <PackageReference Include="Syncfusion.Xamarin.SfTabView" Version="18.2.0.58" />
    <PackageReference Include="Xamarin.Forms" Version="4.8.0.1451" />
    <PackageReference Include="Xamarin.Essentials" Version="1.5.3.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
    <Compile Update="Views\Tuition\TuitionPage.xaml.cs">
      <DependentUpon>TuitionPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Templates\MessageTemplate.xaml.cs">
      <DependentUpon>MessageTemplate.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Message\MessagePage.xaml.cs">
      <DependentUpon>MessagePage.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Controls\LoadingView\LoadingView.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Controls\NavigationView\NavigationView.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Resource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\Account\AccountPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\Account\ChangePasswordPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\Home\HomePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\LeaveRequest\LeaveRequestPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\Login\LoginPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\Login\LoginView.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\Main\MainPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\Notification\NotificationPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Controls\Dialogs\" />
    <Folder Include="ViewModels\News\" />
    <Folder Include="Views\News\" />
    <Folder Include="Views\Tuition\" />
    <Folder Include="ViewModels\Tuition\" />
    <Folder Include="ViewModels\Message\" />
    <Folder Include="Views\Message\" />
    <Folder Include="Views\Survey\" />
    <Folder Include="Views\Templates\" />
  </ItemGroup>
</Project>